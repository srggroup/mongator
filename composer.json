{
	"name": "mongator/mongator",
	"type": "library",
	"description": "Mongator is a easy, powerful and ultrafast ODM for PHP and MongoDB. (forked from Mandango project)",
	"keywords": [
		"odm",
		"mongodb"
	],
	"homepage": "https://mongator.com",
	"license": "MIT",
	"authors": [
		{
			"name": "Máximo Cuadros",
			"email": "mcuadros@gmail.com"
		},
		{
			"name": "Daniel Ripollés",
			"email": "daniel@yunait.com"
		},
		{
			"name": "Pablo Díez",
			"email": "pablodip@gmail.com"
		}
	],
	"repositories": [
		{
			"type": "composer",
			"url": "https://composer.srggroup.dev/"
		}
	],
	"require": {
		"php": "^8.3 | ^8.4",
		"mongator/mondator": "dev-dev-updates",
		"symfony/event-dispatcher": "~7.3.3",
		"mongodb/mongodb": "^2.1.1",
		"laminas/laminas-servicemanager": "~3.23.1",
		"ext-mongodb": "*"
	},
	"require-dev": {
		"srggroup/srg_standards": "v2.0.0",
		"dealerdirect/phpcodesniffer-composer-installer": "^v1.1.2",
		"vimeo/psalm": "^6.13.1"
	},
	"suggest": {
		"ext-apc": "Allow caching on APC",
		"ext-redis": "Allow caching on Redis",
		"ext-memcached": "Allow caching on Memcached"
	},
	"autoload": {
		"psr-0": {
			"Mongator\\": "src/"
		}
	},
	"extra": {
		"phpcodesniffer-search-depth": 5
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
